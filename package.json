{
  "name": "omiverse",
  "version": "1.0.0",
  "description": "typescript with prisma config",
  "main": "dist/main.js",
  "scripts": {
    "lint": "tslint -t stylish --project \"tsconfig.json\"",
    "build": "tsc",
    "start": "node dist/main.js",
    "dev": "nodemon --exec ts-node src/main.ts",
    "watch": "tsc --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fabianferno/express-typescript-boilerplate.git"
  },
  "author": "Fabian Ferno",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/fabianferno/express-typescript-boilerplate/issues"
  },
  "homepage": "https://github.com/fabianferno/express-typescript-boilerplate#readme",
  "dependencies": {
    "axios": "^0.27.2",
    "body-parser": "1.20.2",
    "colors": "1.4.0",
    "cors": "2.8.5",
    "crypto": "1.0.1",
    "dotenv": "16.0.1",
    "ethers": "5.7.2",
    "express": "4.19.2",
    "lodash": "4.17.21",
    "moment": "2.29.4",
    "mongodb": "4.7.0",
    "morgan": "1.10.0",
    "node-cron": "3.0.2",
    "node-fetch": "3.3.0",
    "openai": "4.65.0",
    "single-line-log": "1.1.2",
    "viem": "2.21.2"
  },
  "devDependencies": {
    "@types/body-parser": "1.19.5",
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "@types/morgan": "1.9.9",
    "@types/node": "16.11.26",
    "@types/node-fetch": "2.6.2",
    "nodemon": "2.0.20",
    "ts-node": "10.7.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "4.6.4"
  },
  "nodemonConfig": {
    "ext": "js,json,ejs,jpg,png,gif,svg,css,html,ts,env",
    "watch": [
      "src",
      "dist"
    ]
  },
  "optionalDependencies": {
    "fsevents": "2.3.2"
  }
}